{
  "name": "react-styleguide-generator",
  "version": "0.0.6",
  "description": "Easily generate a style guides from your React components",
  "main": "lib/rsg.js",
  "bin": {
    "rsg": "bin/rsg"
  },
  "directories": {
    "lib": "lib",
    "bin": "bin",
    "example": "example"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rimraf dist/*",
    "prod": "npm run clean && npm run prod:css && npm run prod:js",
    "prod:css": "cssnext -c src/index.css dist/sg.css",
    "prod:js": "browserify -x react src | uglifyjs -m -o dist/sg.js",
    "dev": "npm run dev:css && npm run dev:js",
    "dev:css": "cssnext -sv src/index.css dist/sg.css",
    "dev:js": "browserify -d -x react src -o dist/sg.js"
  },
  "files": [
    "bin",
    "dist",
    "lib",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pocotan001/react-styleguide-generator.git"
  },
  "keywords": [
    "component",
    "documentation",
    "generator",
    "markdown",
    "react",
    "styleguide"
  ],
  "author": "pocotan001",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pocotan001/react-styleguide-generator/issues"
  },
  "homepage": "https://github.com/pocotan001/react-styleguide-generator#readme",
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "peerDependencies": {
    "react": ">=0.13"
  },
  "dependencies": {
    "babelify": "^6.1.2",
    "browserify": "^10.2.4",
    "glob": "^5.0.10",
    "is-url": "^1.2.1",
    "minimist": "^1.1.1",
    "mkdirp": "^0.5.1",
    "mustache": "^2.1.2",
    "object-assign": "^3.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^3.1.17",
    "cssnext": "^1.7.1",
    "eslint": "^0.23.0",
    "eslint-plugin-react": "^2.5.2",
    "highlight.js": "^8.6.0",
    "marked": "^0.3.3",
    "page": "^1.6.3",
    "rimraf": "^2.4.0",
    "uglify-js": "^2.4.23"
  }
}
